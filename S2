第一案
-------------------------------------------------------------
#include <stdio.h>
int main(void){

    int n,i,j,k;
    scanf("%d",&n);
    long ary[100000];
    for(i = 0;i < n;i++) {
        scanf("%d",&ary[i]);
    }
    int s = 0;
    for(i = 0;i < n - 2;i++) {
        for(j = i + 1;j < n - 1;j++) {
            for(k = j + 1;k < n;k++) {
                if((ary[i] + ary[j] + ary[k]) % 7 == 0) {
                    s++;
                }
            }
        }
    }
    printf("%d",s);
    
    return 0;
}
-----------------------------------------------------------
理論上可能ではあるが最大データ数が100000のため100000×99999×99998回もループし、時間がかかりすぎるためやり直し


第二案
-----------------------------------------------------------------------------
#include <stdio.h>
int main(void){
    // 自分の得意な言語で
    // Let's チャレンジ！！

    int n,i,j;
    unsigned long k;
    scanf("%d",&n);
    int ary[7] = {};
    for(i = 0;i < n ;i++) {
        scanf("%lu",&k);
        j = k % 7;
        ary[j] = ary[j] + 1;
    }
    unsigned long m;
    unsigned long s = 0;
    for(i = 0;i < 7;i++) {
        for(j = i;j < 7;j++) {
            for(k = j;k < 7;k++) {
                if((i + j + k) % 7 == 0) {
                    if(i == j && i == k && j == k){
                        m = ary[i] * (ary[j] - 1) * (ary[k] - 2) / 6;
                        s = s + m;
                    } else if(i == j){
                        m = ary[i] * (ary[j] - 1) * ary[k] / 2;
                        s = s + m;
                    } else if(j == k){
                        m = ary[i] * ary[j] * (ary[k] - 1) / 2;
                        s = s + m;
                    } else {
                        m = ary[i] * ary[j] * ary[k];
                        s = s + m;
                    }
                }
            }
        }
    }
    printf("%lu",s);
    
    return 0;
}
--------------------------------------------------------------------------------
